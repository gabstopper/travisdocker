[run]
omit = 
        __init__.py
        *counter.py*
        *locators.py*
        *parser.py*
        *test.py*
        *shortcuts.py*

[report]
# Regexes for lines to exclude from consideration
exclude_lines =

    # Don't complain about missing debug-only code:
    def __repr__
    def __str__
    __str__
    pass
    raise NotImplementedError
    except ImportError
